[alias]
  a = add
  b = branch -v
  bd = branch -vD
  co = checkout
  logshort = log --pretty=format:\"%h %s\"
  rmb = !git branch | grep -v " master$" | xargs git branch -D
  s = status
  superc = !git clean -dfx && git checkout .

[push]
	default = simple

# Private things like username, email, etc. go in a different file.
[include]
  path = ~/.gitconfig.local
